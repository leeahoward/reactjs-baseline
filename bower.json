{
  "name": "\"name\": \"reactjs-baseline\",",
  "version": "0.0.0",
  "authors": [
    "Mark Lussier <mlussier@gmail.com>"
  ],
  "description": "Opinionated starting point boilerplate for buiding amazing things with ReactJS",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "director": "~1.2.1",
    "react": "~0.8.0",
    "zepto": "~1.0.0",
    "normalize-css": "~2.1.3"
  },
  "exportsOverride":{
    "director":{
      "js":"build/**/*.js"
    },
    "react":{
      "js":"**/*.js"
    },
    "zepto":{
      "js":"**/*.js"
    },
    "normalize-css":{
      "css":"**/*.css"
    }
  }
}
